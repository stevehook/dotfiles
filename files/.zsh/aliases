alias lg="ls -Glah"
alias ll="ls -lah"
alias vim="/Applications/MacVim.app/Contents/MacOS/Vim"
alias be="bundle exec"
alias h="heroku"
alias g="git"
alias pgstart="pg_ctl -D ~/Library/Application\ Support/Postgres/var-9.3 -l ~/Library/Application\ Support/Postgres/var-9.3/server.log start"
alias pgstop="pg_ctl -D ~/Library/Application\ Support/Postgres/var-9.3 stop -s -m fast"
alias msstart="mysql.server start"
alias msstop="mysql.server stop"
alias git-daemon="git daemon --base-path=/Users/stevehook/Workspace --export-all &"
alias esstart="launchctl load -wF ~/Library/LaunchAgents/homebrew.mxcl.elasticsearch.plist"
alias esstop="launchctl unload -wF ~/Library/LaunchAgents/homebrew.mxcl.elasticsearch.plist"
alias gitk="gitk --all &"
alias gg='git grep --break --heading --line-number --color=auto'
alias foreman=forego
